{
  "baseUrl": "./",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/utils/*": [
        "utils/*"
      ],
      "@/types/*": [
        "types/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/assets/*": [
        "assets/*"
      ],
      "@/store/*": [
        "store/*"
      ],
      "@/services/*": [
        "services/*"
      ],
      "@/models/*": [
        "models/*"
      ],
      "@/public/*": [
        "public/*"
      ],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/index.jsx",
    "components/helpers/FancyBox.jsx",
    "pages/auth/login.jsx",
    "pages/auth/register.jsx",
    "types/design.model.js",
    "pages/api/designs/index.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "resolveJsonModule": true,
  "esModuleInterop": true,
  "jsx": "react-jsx",
  "jsxImportSource": "@emotion/react"
}